    <!-- list of 3- or 4-letter mnemonics for a 10-key numeric keypad -->
    <string-array translatable="false" name="lockscreen_num_pad_klondike">
        <item>+</item><!-- 0 -->
        <item></item><!-- 1 -->
        <item>ABC</item><!-- 2 -->
        <item>DEF</item><!-- 3 -->
        <item>GHI</item><!-- 4 -->
        <item>JKL</item><!-- 5 -->
        <item>MNO</item><!-- 6 -->
        <item>PQRS</item><!-- 7 -->
        <item>TUV</item><!-- 8 -->
        <item>WXYZ</item><!-- 9 -->
    </string-array>

